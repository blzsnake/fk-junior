extends /blocks/layout-default/layout-default
include /blocks/banner/banner
include /blocks/contacts/contacts
include /blocks/gallery/gallery
include /blocks/news-box/news-box

block head
	- var pageTitle = 'ФК ДФШ ЮНИОР';


block content

	- banner = getData('banner');
	- gallery = getData('gallery');
	- contacts = getData('contacts');

	main.main
		+banner(banner)
		+news-box
		+gallery(gallery)
		+contacts(contacts)

