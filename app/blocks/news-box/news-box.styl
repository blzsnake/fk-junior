.news-box
	background $blue-dk
	padding-top $(25px)
	padding-bottom $(35px)
	
	&__title
		font-size $(36px)
		text-align center
		margin-bottom $(45px)
	
	&__list		
		display flex
		justify-content center
		flex-wrap wrap
		margin-bottom 10px
		
	&__more
		font-size $(50px)
		color $grey
		text-align center