mixin contacts(data)

	-data = data || []

	+b('section').contacts
		+e.data
			+e.title Контактные данные
			+e('ul').list
				each item in data
					+e('li').item
						span= item.title
						| : 
						span= item.data
		+e.map
			iframe(src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d4942.3822450752305!2d36.19047781242674!3d51.7295395363639!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sru!2sru!4v1457557437182" frameborder="0" style="border:0" allowfullscreen)